{
  "dataset_reader": {
    "name": "odqa_reader",
    "data_path": "{DOWNLOADS_PATH}/odqa/ruwiki_test",
    "save_path": "{DOWNLOADS_PATH}/odqa/ruwiki_test.db",
    "dataset_format": "txt"
  },
  "dataset_iterator": {
    "name": "sqlite_iterator",
    "shuffle": false,
    "load_path": "{DOWNLOADS_PATH}/odqa/ruwiki_test.db"
  },
  "chainer": {
    "in": [
      "x"
    ],
    "out": [
      "y"
    ],
    "pipe": [
      {
        "name": "hashing_tfidf_vectorizer",
        "id": "vectorizer",
        "fit_on_batch": [
          "x"
        ],
        "save_path": "{DOWNLOADS_PATH}/odqa/ruwiki_test_tfidf.npz",
        "load_path": "{DOWNLOADS_PATH}/odqa/ruwiki_test_tfidf.npz",
        "tokenizer": {
          "name": "ru_tokenizer",
          "lemmas": true,
          "ngram_range": [
            1,
            2
          ]
        }
      },
      {
        "name": "tfidf_ranker",
        "top_n": 5,
        "in": [
          "x"
        ],
        "out": [
          "y",
          "score"
        ],
        "vectorizer": "#vectorizer"
      }
    ]
  },
  "train": {
    "validate_best": false,
    "test_best": false,
    "batch_size": 2
  },
  "metadata": {
    "variables": {
      "ROOT_PATH": "~/.deeppavlov",
      "DOWNLOADS_PATH": "{ROOT_PATH}/downloads",
      "MODELS_PATH": "{ROOT_PATH}/models"
    },
    "requirements": [],
    "labels": {
      "server_utils": "Ranker"
    },
    "download": [
      {
        "url": "http://files.deeppavlov.ai/datasets/wikipedia/ruwiki_test.tar.gz",
        "subdir": "{DOWNLOADS_PATH}/odqa"
      }
    ]
  }
}
