{
  "chainer": {
    "in": [
      "question_raw"
    ],
    "out": [
      "best_answer"
    ],
    "pipe": [
      {
        "config_path": "../deeppavlov/configs/doc_retrieval/en_ranker_tfidf_wiki_test.json",
        "in": [
          "question_raw"
        ],
        "out": [
          "doc_ids"
        ]
      },
      {
        "name": "wiki_sqlite_vocab",
        "in": [
          "doc_ids"
        ],
        "out": [
          "context_raw"
        ],
        "join_docs": false,
        "shuffle": false,
        "load_path": "odqa/enwiki_test.db"
      },
      {
        "name": "document_chunker",
        "in": [
          "context_raw"
        ],
        "out": [
          "chunks"
        ],
        "flatten_result": true
      },
      {
        "name": "string_multiplier",
        "in": [
          "question_raw",
          "chunks"
        ],
        "out": [
          "questions"
        ]
      },
      {
        "name": "logit_ranker",
        "squad_model": {
          "config_path": "../deeppavlov/configs/squad/squad.json"
        },
        "in": [
          "chunks",
          "questions"
        ],
        "out": [
          "best_answer"
        ]
      }
    ]
  },
  "metadata": {
    "requirements": [
      "{DEEPPAVLOV_PATH}/requirements/tf.txt",
      "{DEEPPAVLOV_PATH}/requirements/spacy.txt",
      "{DEEPPAVLOV_PATH}/requirements/en_core_web_sm.txt"
    ],
    "labels": {
      "server_utils": "ODQA"
    },
    "download": [
      "http://files.deeppavlov.ai/deeppavlov_data/odqa_test.tar.gz"
    ]
  }
}